{
  "name": "pb138-insurance",
  "version": "1.0.0",
  "description": "Projekt na PB138",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:show": "prettier -l .",
    "frontend-dev": "cd frontend && npm run dev",
    "backend-dev": "cd backend && npm run dev-gendoc",
    "dev": "concurrently \"npm run frontend-dev\" \"npm run backend-dev\"",
    "frontend-prod": "cd frontend && npm run build && npm run prod",
    "backend-prod": "cd backend && npm run prod",
    "prod": "concurrently \"npm run frontend-prod\" \"npm run backend-prod\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "prettier": "^2.6.2"
  }
}
